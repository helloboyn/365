class Solution {
public:
    int removePalindromeSub(string s) {
        if(s.size()==0)return 0; //for empty stings we can write this way
        int i=0;
        int j=s.size()-1;
        while(i<j){              //if not empty the 
            if(s[i]==s[j]){      //ssequence is working
                i++;
                j--;
            }       
                else return 2;   
        }
        return 1;
    }
};
